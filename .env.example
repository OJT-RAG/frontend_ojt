# Frontend environment variables
# Copy this file to `.env` (same folder) and fill in your values.

# Backend base URL (used by signup and other API calls)
REACT_APP_API_BASE_URL=http://localhost:7031

# Signup endpoint (relative to base)
REACT_APP_SIGNUP_ENDPOINT=/api/user/create

# Majors endpoint (relative to base)
REACT_APP_MAJOR_GETALL=/api/Major/getAll

# RAG backend base URL (used for chat and document management)
REACT_APP_RAG_API_BASE_URL=https://ojt-rag-python.onrender.com

# Google OAuth (required)
# Obtain the Client ID from Google Cloud Console (OAuth 2.0 Client IDs)
REACT_APP_GOOGLE_CLIENT_ID=YOUR_CLIENT_ID.apps.googleusercontent.com

# Must exactly match the redirect URI registered in Google Cloud
REACT_APP_GOOGLE_REDIRECT_URI=https://localhost:7031/auth/google/callback
